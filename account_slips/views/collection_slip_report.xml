<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="action_report_collection_slip"
        string="Imprimir Contraseña"
        model="account.slip"
        report_type="qweb-pdf"
        file="account_slips.report_collection_slip"
        name="account_slips.report_collection_slip"
    />
    <record id="report_collection_slip_paperformat" model="report.paperformat">
	    <field name="name">Contraseña de Pago</field>
	    <field name="default" eval="True"/>
	    <field name="format">custom</field>
	    <field name="page_height">130</field>
	    <field name="page_width">100</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">7</field>
	    <field name="margin_bottom">7</field>
	    <field name="margin_left">7</field>
	    <field name="margin_right">7</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">5</field>
	    <field name="dpi">150</field>
	    <field name="report_ids" eval="[(4, ref('action_report_collection_slip'))]"/>
	</record>

	    <report
        id="action_report_collection_slip_other"
        string="Imprimir Quedan"
        model="account.slip"
        report_type="qweb-pdf"
        file="account_slips.report_collection_slip_other"
        name="account_slips.report_collection_slip_other"
    />

    <record id="report_collection_slip_paperformat_other" model="report.paperformat">
	    <field name="name">Contraseña de Pago Other</field>
	    <field name="default" eval="True"/>
	    <field name="format">Letter</field>
	  
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">7</field>
	    <field name="margin_bottom">7</field>
	    <field name="margin_left">7</field>
	    <field name="margin_right">7</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">7</field>
	    <field name="dpi">90</field>
	    <field name="report_ids" eval="[(4, ref('action_report_collection_slip_other'))]"/>
	</record>
</odoo>
