<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Categoria de grupos-->
        <record id="category_mrp_workforce" model="ir.module.category">
            <field name="name">Mano de obra fabricacion</field>
            <field name="description">Categoria para mano de obra fabricacion</field>
            <field name="sequence">10</field>
        </record>
        <!--Grupos de seguridad-->
        <record id="group_mrp_workforce_user" model="res.groups">
            <field name="name">Crear y editar registros</field>
            <field name="category_id" ref="mrp_workforce.category_mrp_workforce"/>
        </record>
        <record id="group_mrp_workforce_manager" model="res.groups">
            <field name="name">Todos los permisos</field>
            <field name="category_id" ref="mrp_workforce.category_mrp_workforce"/>
        </record>
        <!--Rueles multicompaÃ±ia-->
        <record id="mrp_workforce_multi_company_rule" model="ir.rule">
            <field name="name">mrp workforce multi company rule</field>
            <field name="model_id" ref="model_mrp_workforce" />
            <field name="global" eval="True" />
            <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
        </record>
    </data>
</odoo>