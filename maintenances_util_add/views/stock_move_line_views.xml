<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_view_move_line_tree" model="ir.ui.view">
            <field name="name">view.move.line.inherit</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_move_line_tree"/>
            <field name="arch" type="xml">
                <field name="qty_done" position="after">
                    <field name="product_tmpl_id" invisible="1"/>
                    <field name="currency_id" invisible="1" />
                    <field name="cost_component" optional="hide" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                    <field name="maintenance_equipment_id2" optional="hide"/>
                    <field name="cost_qty" widget='monetary' options="{'currency_field': 'currency_id'}" optional="hide" sum="Total"/>
                </field>                
            </field>
        </record>
    </data>
</odoo>