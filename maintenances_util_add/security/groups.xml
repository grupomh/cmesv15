<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
    <!--Categoria de grupos-->
        <record id="category_maintenances_bom" model="ir.module.category">
            <field name="name">Inventario de mantenimiento</field>
            <field name="description">inventarios de mantenimiento</field>
            <field name="sequence">10</field>
        </record>
    <!--Grupos de seguridad-->
        <record id="group_maintenances_bom_user" model="res.groups">
            <field name="name">Permisos de creacion y edicion</field>
            <field name="category_id" ref="maintenances_util_add.category_maintenances_bom"/>
        </record>
        <record id="group_maintenances_bom_manager" model="res.groups">
            <field name="name">Todos los permisos</field>
            <field name="category_id" ref="maintenances_util_add.category_maintenances_bom"/>
        </record>
        <!--Rules multicompaÃ±ia-->
        <record model="ir.rule" id="maintenances_bom_multi_company_rule">
            <field name="name">maintenances bom multi company rules</field>
            <field name="model_id" ref="model_maintenances_bom"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
        </record>

    </data>
</odoo>