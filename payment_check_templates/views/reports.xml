<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="action_report_promerica_template"
        string="Voucher Promerica"
        model="account.payment"
        report_type="qweb-pdf"
        file="payment_check_templates.report_promerica_voucher"
        name="payment_check_templates.report_promerica_voucher"
        print_report_name="object.name"
    />

    <report
        id="action_report_banrural_template"
        string="Voucher Banrural"
        model="account.payment"
        report_type="qweb-pdf"
        file="payment_check_templates.report_banrural_voucher"
        name="payment_check_templates.report_banrural_voucher"
        print_report_name="object.name"
    />
    <report
        id="action_report_city_template"
        string="Voucher Citybank"
        model="account.payment"
        report_type="qweb-pdf"
        file="payment_check_templates.report_city_voucher"
        name="payment_check_templates.report_city_voucher"
        print_report_name="object.name"
    />
    <record id="report_promerica_paperformat" model="report.paperformat">
	    <field name="name">Voucher Promerica</field>
	    <field name="default" eval="True"/>
	    <field name="format">Letter</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">20</field>
	    <field name="margin_bottom">7</field>
	    <field name="margin_left">7</field>
	    <field name="margin_right">7</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">5</field>
	    <field name="dpi">90</field>
	    <field name="report_ids" eval="[(4, ref('action_report_promerica_template'))]"/>
	</record>

    <record id="report_banrural_paperformat" model="report.paperformat">
	    <field name="name">Voucher Banrural</field>
	    <field name="default" eval="True"/>
	    <field name="format">Letter</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">25</field>
	    <field name="margin_bottom">10</field>
	    <field name="margin_left">22</field>
	    <field name="margin_right">22</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">5</field>
	    <field name="dpi">90</field>
	    <field name="report_ids" eval="[(4, ref('action_report_banrural_template'))]"/>
	</record>

    <record id="report_city_paperformat" model="report.paperformat">
	    <field name="name">Voucher Citybank</field>
	    <field name="default" eval="True"/>
	    <field name="format">Letter</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">25</field>
	    <field name="margin_bottom">10</field>
	    <field name="margin_left">22</field>
	    <field name="margin_right">22</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">5</field>
	    <field name="dpi">90</field>
	    <field name="report_ids" eval="[(4, ref('action_report_city_template'))]"/>
	</record>
</odoo>
