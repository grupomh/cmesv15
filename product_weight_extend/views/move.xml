<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_weight_account_move" model="ir.ui.view">
            <field name="name">product.weight.account.move</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='quantity']" position="after">
                    <field name="weight" groups="product_weight_extend.group_weight"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>